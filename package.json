{
  "name": "@adsisto/clavis",
  "productName": "Clavis",
  "version": "0.0.1",
  "description": "Clavis is a simple desktop security key application. It allows you to generate a public/private key pair and use it to sign JWT tokens for secure authentication with web services.",
  "homepage": "https://adsisto.org",
  "license": "GPL-3.0-only",
  "main": "dist/main.js",
  "scripts": {
    "start": "electron .",
    "build:go": "cd helper && go build -o ../bin/helper .",
    "build:scripts-dev": "cross-env NODE_ENV=development webpack --config webpack.config.js",
    "build:scripts-prod": "cross-env NODE_ENV=production webpack --config webpack.config.js",
    "build:app": "electron-builder",
    "package": "yarn run build:go && yarn run build:scripts-prod && yarn run build:app"
  },
  "keywords": [],
  "author": {
    "name": "Andrew Ying",
    "email": "hi@andrewying.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/adsisto/clavis"
  },
  "browserslist": [
    "electron 6.0"
  ],
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@babel/plugin-proposal-function-bind": "^7.0.0",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@bugsnag/js": "^6.4.0",
    "@bugsnag/plugin-react": "^6.4.0",
    "autoprefixer": "^9.6.1",
    "babel-loader": "^8.0.6",
    "cross-env": "^6.0.3",
    "css-loader": "^3.1.0",
    "electron": "6.0.0",
    "electron-builder": "^21.2.0",
    "electron-debug": "^3.0.0",
    "electron-devtools-installer": "^2.2.4",
    "electron-log": "^3.0.8",
    "electron-notarize": "^0.1.1",
    "electron-squirrel-startup": "^1.0.0",
    "electron-store": "^5.0.0",
    "electron-updater": "^4.1.2",
    "eslint": "^6.1.0",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-security": "^1.4.0",
    "extract-css-chunks-webpack-plugin": "^4.5.6",
    "file-loader": "^4.2.0",
