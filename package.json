{
  "name": "@adsisto/clavis",
  "productName": "Clavis",
  "version": "0.0.1",
  "description": "Clavis is a simple desktop security key application. It allows you to generate a public/private key pair and use it to sign JWT tokens for secure authentication with web services.",
  "homepage": "https://adsisto.org",
  "license": "GPL-3.0-only",
  "main": "dist/index.js",
  "scripts": {
    "start": "electron .",
    "build:go": "cd helper && go build -o ../bin/helper .",
    "build:scripts-dev": "cross-env NODE_ENV=development webpack --config webpack.config.js",
    "build:scripts-prod": "cross-env NODE_ENV=production webpack --config webpack.config.js",
    "build:app": "electron-builder",
    "package": "yarn run build:go && yarn run build:scripts-prod && yarn run build:app"
  },
  "keywords": [],
  "author": {
    "name": "Andrew Ying",
    "email": "hi@andrewying.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/adsisto/clavis"
  },
  "browserslist": [
    "electron 4.1"
  ],
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-proposal-decorators": "^7.4.0",
    "@babel/plugin-proposal-function-bind": "^7.0.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "autoprefixer": "^9.3.1",
    "babel-loader": "^8.0.2",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "electron": "4.1.4",
    "electron-builder": "^20.39.0",
    "electron-debug": "^2.2.0",
    "electron-devtools-installer": "^2.2.4",
    "electron-log": "^3.0.5",
    "electron-squirrel-startup": "^1.0.0",
    "electron-store": "^3.2.0",
    "electron-updater": "^4.0.6",
    "extract-css-chunks-webpack-plugin": "^4.3.2",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "node-sass": "^4.0.0",
    "postcss-functions": "^3.0.0",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.6.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-router-dom": "^5.0.0",
    "sass-loader": "^7.1.0",
    "source-map-support": "^0.5.12",
    "style-loader": "^0.23.1",
    "terser-webpack-plugin": "^1.2.3",
    "validate.js": "^0.12.0",
    "webpack": "^4.25.1",
    "webpack-cli": "^3.1.2"
  }
}
