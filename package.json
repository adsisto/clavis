{
  "name": "@adsisto/clavis",
  "productName": "Clavis",
  "version": "1.0.0",
  "description": "",
  "license": "GPL-3.0-only",
  "main": "dist/index.js",
  "scripts": {
    "start": "electron-forge start",
    "build:dev": "cross-env NODE_ENV=development webpack --config webpack.config.js",
    "build:prod": "cross-env NODE_ENV=production webpack --config webpack.config.js",
    "package": "yarn build:prod && electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "echo \"No linting configured\""
  },
  "keywords": [],
  "author": "andrewying",
  "browserslist": [
    "electron 4.1"
  ],
  "config": {
    "forge": {
      "packagerConfig": {
        "appCopyright": "Copyright (c) Andrew Ying 2019.",
        "ignore": [
          "/^\\/?\\.idea/",
          "/^\\/?\\helper/",
          "/^\\/?\\src/"
        ],
        "appBundleId": "org.adsisto.clavis"
      },
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-dmg",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  },
  "dependencies": {
    "electron-squirrel-startup": "^1.0.0",
    "electron-store": "^3.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-proposal-decorators": "^7.4.0",
    "@babel/plugin-proposal-function-bind": "^7.0.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@electron-forge/cli": "6.0.0-beta.34",
    "@electron-forge/maker-deb": "6.0.0-beta.34",
    "@electron-forge/maker-dmg": "^6.0.0-beta.34",
    "@electron-forge/maker-rpm": "6.0.0-beta.34",
    "@electron-forge/maker-squirrel": "6.0.0-beta.34",
    "autoprefixer": "^9.3.1",
    "babel-loader": "^8.0.2",
    "copy-webpack-plugin": "^5.0.2",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.1",
    "electron": "4.1.4",
    "electron-debug": "^2.2.0",
    "electron-devtools-installer": "^2.2.4",
    "extract-css-chunks-webpack-plugin": "^4.3.2",
    "file-loader": "^3.0.1",
    "node-sass": "^4.0.0",
    "postcss-functions": "^3.0.0",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.6.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-router-dom": "^5.0.0",
    "sass-loader": "^7.1.0",
    "source-map-support": "^0.5.12",
    "style-loader": "^0.23.1",
    "uglifyjs-webpack-plugin": "^2.1.2",
    "webpack": "^4.25.1",
    "webpack-cli": "^3.1.2"
  }
