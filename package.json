{
  "name": "@adsisto/clavis",
  "productName": "Clavis",
  "version": "1.0.0",
  "description": "",
  "homepage": "https://adsisto.org",
  "license": "GPL-3.0-only",
  "main": "dist/index.js",
  "scripts": {
    "start": "electron .",
    "build:go": "cd helper && go build -o ../bin/helper .",
    "build:scripts-dev": "cross-env NODE_ENV=development webpack --config webpack.config.js",
    "build:scripts-prod": "cross-env NODE_ENV=production webpack --config webpack.config.js",
    "build:app": "CSC_NAME=\"Andrew Ying\" electron-builder",
    "package": "yarn run build:go && yarn run build:scripts-prod && yarn run build:app"
  },
  "keywords": [],
  "author": {
    "name": "Andrew Ying",
    "email": "hi@andrewying.com"
  },
  "browserslist": [
    "electron 4.1"
  ],
  "build": {
    "appId": "org.adsisto.clavis",
    "directories": {
      "output": "release"
    },
    "files": [
      "**/*",
      "!node_modules/**/*",
      "!src/**/*",
      "!helper/**/*",
      "!.editorconfig",
      "!{README.md,LICENSE.md}",
      "!**/*.{iml,o,hprof,orig,pyc,pyo,rbc,swp,csproj,sln,xproj}",
      "!**/._*",
      "!**/{.DS_Store,.git,.gitignore,.gitattributes,.babelrc,.editorconfig}",
      "!**/{__pycache__,thumbs.db,.flowconfig,.idea,.vs,.nyc_output}",
      "!*.config.js",
      "!{npm-debug.log,yarn.lock,.yarn-integrity,.yarn-metadata.json}"
    ],
    "generateUpdatesFilesForAllChannels": true,
    "remoteBuild": false
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-proposal-decorators": "^7.4.0",
    "@babel/plugin-proposal-function-bind": "^7.0.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "autoprefixer": "^9.3.1",
    "babel-loader": "^8.0.2",
    "copy-webpack-plugin": "^5.0.2",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "electron": "4.1.4",
    "electron-builder": "^20.39.0",
    "electron-debug": "^2.2.0",
    "electron-devtools-installer": "^2.2.4",
    "electron-log": "^3.0.5",
    "electron-squirrel-startup": "^1.0.0",
    "electron-store": "^3.2.0",
    "electron-updater": "^4.0.6",
    "extract-css-chunks-webpack-plugin": "^4.3.2",
    "file-loader": "^3.0.1",
    "node-sass": "^4.0.0",
    "postcss-functions": "^3.0.0",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.6.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-router-dom": "^5.0.0",
    "sass-loader": "^7.1.0",
    "source-map-support": "^0.5.12",
    "style-loader": "^0.23.1",
    "terser-webpack-plugin": "^1.2.3",
    "validate.js": "^0.12.0",
    "webpack": "^4.25.1",
    "webpack-cli": "^3.1.2"
  }
}
