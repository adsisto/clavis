    "react-dom": "^16.8.6",
    "react-router-dom": "^5.0.1",
    "sass-loader": "^7.1.0",
    "source-map-support": "^0.5.12",
    "style-loader": "^0.23.1",
    "stylelint": "^10.1.0",
    "stylelint-config-sass-guidelines": "^6.0.0",
    "terser-webpack-plugin": "^1.3.0",
    "uuid": "^3.3.2",
    "validate.js": "^0.13.1",
    "webpack": "^4.35.0",
    "webpack-cli": "^3.3.4"
  }
}
